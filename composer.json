{
    "name": "league/playground",
    "description": "My Playground for everything PHP",
    "keywords": [
        "league",
        "playground"
    ],
    "homepage": "https://github.com/thephpleague/playground",
    "license": "MIT",
    "authors": [
        {
            "name": "Tung Ha",
            "email": "tunght13488@gmail.com",
            "homepage": "https://github.com/tunght13488",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "League\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Skeleton\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
